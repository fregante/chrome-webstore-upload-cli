{
  "name": "chrome-webstore-upload-cli",
  "version": "1.2.2",
  "license": "MIT",
  "description": "CLI Utility to quickly upload + publish extensions to the Chrome Web Store",
  "repository": "fregante/chrome-webstore-upload-cli",
  "bin": {
    "webstore": "index.js"
  },
  "type": "module",
  "scripts": {
    "test": "xo && ava"
  },
  "keywords": [
    "chrome",
    "publish",
    "webstore",
    "extension",
    "deploy",
    "update",
    "upload",
    "cli"
  ],
  "author": "Andrew Levine",
  "devDependencies": {
    "ava": "^3.15.0",
    "execa": "^5.1.1",
    "is-stream": "^3.0.0",
    "xo": "^0.44.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chrome-webstore-upload": "^0.5.0",
    "junk": "^4.0.0",
    "meow": "^10.1.1",
    "ora": "^6.0.1",
    "recursive-readdir": "^2.2.2",
    "yazl": "^2.5.1"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "contributors": [
    "Andrew Levine",
    "Federico Brigante <me@fregante.com> (https://fregante.com)"
  ],
  "ava": {
    "files": [
      "!test/fixtures/**",
      "!test/helpers/**"
    ]
  },
  "xo": {
    "space": 4,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "camelcase": "off",
      "func-names": "off",
      "node/prefer-global/process": "off",
      "promise/prefer-await-to-then": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-process-exit": "off"
    }
  }
}
