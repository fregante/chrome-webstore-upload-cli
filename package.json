{
  "name": "chrome-webstore-upload-cli",
  "version": "3.1.0",
  "description": "CLI tool to upload Chrome Extensions to the Chrome Web Store",
  "keywords": [
    "chrome",
    "publish",
    "webstore",
    "store",
    "extension",
    "deploy",
    "update",
    "upload",
    "cli"
  ],
  "repository": "fregante/chrome-webstore-upload-cli",
  "funding": "https://github.com/sponsors/fregante",
  "license": "MIT",
  "author": "Federico Brigante <me@fregante.com> (https://fregante.com)",
  "contributors": [
    "Andrew Levine"
  ],
  "type": "module",
  "bin": {
    "chrome-webstore-upload": "distribution/cli.js"
  },
  "scripts": {
    "test": "xo && ava",
    "build": "tsc",
    "watch": "tsc -w",
    "prepack": "npm run build"
  },
  "xo": {
    "ignore": [
      "test/fixtures"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "space": 4
  },
  "ava": {
    "files": [
      "!test/fixtures/**",
      "!test/helpers/**"
    ]
  },
  "dependencies": {
    "chrome-webstore-upload": "^3.0.0",
    "junk": "^4.0.1",
    "meow": "^12.1.1",
    "ora": "^7.0.1",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^5.0.0",
    "@types/yazl": "^2.4.5",
    "ava": "^6.0.1",
    "execa": "^8.0.1",
    "is-stream": "^3.0.0",
    "mock-fs": "^5.2.0",
    "typescript": "^5.3.3",
    "xo": "^0.56.0"
  },
  "engines": {
    "node": ">=18.17"
  }
}
